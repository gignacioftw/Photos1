<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.photos.Controllers.openController">
    <Label fx:id="nameLabel" layoutX="21.0" layoutY="23.0" text="Hello: " />
   <Label fx:id="albumTitle" alignment="CENTER" contentDisplay="CENTER" layoutX="203.0" layoutY="16.0" prefHeight="48.0" prefWidth="194.0" text="Album Name" textAlignment="CENTER">
      <font>
         <Font size="25.0" />
      </font>
   </Label>
   <ScrollPane layoutX="198.0" layoutY="75.0" prefHeight="296.0" prefWidth="388.0">
      <content>
         <TilePane fx:id="vbox" prefColumns="10" prefHeight="394.0" prefRows="4" prefWidth="376.0" style="-fx-border-color: blue;">
            <cursor>
               <Cursor fx:constant="DEFAULT" />
            </cursor>
         </TilePane>
      </content>
   </ScrollPane>
   <Button fx:id="addPhoto" layoutX="38.0" layoutY="93.0" mnemonicParsing="false" onAction="#add" text="Add Photo" />
   <Label fx:id="addLabel" alignment="CENTER" layoutX="139.0" layoutY="82.0" prefHeight="48.0" prefWidth="228.0" textAlignment="CENTER" textFill="RED" />

</AnchorPane>
