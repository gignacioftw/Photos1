      </content>
   </ScrollPane>
   <Button fx:id="deselectButton" layoutX="324.0" layoutY="360.0" mnemonicParsing="false" onAction="#deselect" prefHeight="25.0" prefWidth="137.0" text="Deselect" textAlignment="CENTER" />

