<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.photos.Controllers.userHomeController">
    <Label fx:id="nameLabel" layoutX="21.0" layoutY="23.0" text="Hello: " />
   <TilePane fx:id="vbox" layoutX="386.0" layoutY="25.0" prefHeight="350.0" prefWidth="200.0" style="-fx-border-color: blue;" />
   <Button fx:id="createAlbum" layoutX="62.0" layoutY="94.0" mnemonicParsing="false" onAction="#create" text="Create Album" />
   <TextField fx:id="albumInput" layoutX="167.0" layoutY="94.0" />
   <Button fx:id="deleteAlbum" layoutX="62.0" layoutY="145.0" mnemonicParsing="false" onAction="#delete" text="Delete Album" />
   <Label fx:id="deleteLabel" alignment="TOP_LEFT" layoutX="167.0" layoutY="145.0" prefHeight="42.0" prefWidth="200.0" textAlignment="CENTER" textFill="RED" />
   <Button fx:id="deleteCancel" layoutX="300.0" layoutY="188.0" mnemonicParsing="false" onAction="#deleteCancel" text="Cancel" />
   <Button fx:id="deleteConfirm" layoutX="183.0" layoutY="188.0" mnemonicParsing="false" onAction="#deleteConfirm" text="Confirm" />
</AnchorPane>
